# 阿里千问API配置（必需）
DASHSCOPE_API_KEY=your-dashscope-api-key

# AI模型配置
QWEN_MODEL=qwen-max

# 高德地图API配置
AMAP_API_KEY=your-amap-api-key
AMAP_API_BASE_URL=https://restapi.amap.com/v3
AMAP_REQUEST_TIMEOUT=10

# 语音识别配置
SPEECH_RECOGNITION_LANGUAGE=zh-CN
SPEECH_TIMEOUT=5

# 科大讯飞语音识别配置
XFYUN_APP_ID=your-xfyun-app-id
XFYUN_API_SECRET=your-xfyun-api-secret
XFYUN_API_KEY=your-xfyun-api-key
USE_XFYUN_ASR=true

# 七牛云MCP SERVER配置
OPENAI_BASE_URL=https://your-qiniu-mcp-server.com
OPENAI_API_KEY=your-qiniu-mcp-api-key
MCP_MODEL=deepseek-v3-tool
USE_QINIU_MCP=false

# 导航模式配置
NAVIGATION_MODE=browser
BROWSER_NAVIGATION_ENABLED=true

# 默认城市配置
DEFAULT_CITY=深圳市